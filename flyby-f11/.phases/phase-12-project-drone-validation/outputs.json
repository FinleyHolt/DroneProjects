{
  "phase": "phase-12-project-drone-validation",
  "required_outputs": {
    "deployment_configs": {
      "path": "deployment/project-drone/",
      "type": "directory",
      "description": "Deployment configuration for project-drone",
      "verification": "directory_exists"
    },
    "arm_containers": {
      "name": "flyby-f11-execution:latest-arm64",
      "type": "podman_image",
      "description": "ARM64 container image for Jetson",
      "verification": "podman images | grep arm64 || true"
    },
    "flight_logs": {
      "path": "hardware_validation/project-drone/logs/",
      "type": "directory",
      "description": "Flight test logs and telemetry",
      "verification": "directory_exists"
    },
    "performance_report": {
      "path": "hardware_validation/project-drone/performance.json",
      "type": "file",
      "description": "Hardware performance measurements",
      "verification": "file_exists"
    },
    "test_results": {
      "path": "hardware_validation/project-drone/test_results.json",
      "type": "file",
      "description": "Flight test success/failure results",
      "verification": "file_exists"
    },
    "sim_to_real_notes": {
      "path": "hardware_validation/project-drone/SIM_TO_REAL.md",
      "type": "file",
      "description": "Documentation of sim-to-real transfer issues",
      "verification": "file_exists"
    }
  },
  "success_indicators": {
    "hardware_deployed": "System runs on Jetson Orin Nano Super",
    "sensors_working": "T265 and D455 provide valid data",
    "flights_successful": "10+ autonomous flights completed",
    "collision_free": "Zero collisions during testing",
    "gps_denied_works": "5+ minutes GPS-denied navigation"
  },
  "metrics": {
    "successful_flights": "Number of successful autonomous flights",
    "total_flight_time_minutes": "Cumulative flight time",
    "collision_count": "Number of collisions (should be 0)",
    "cpu_utilization_percent": "Average CPU usage",
    "gpu_utilization_percent": "Average GPU usage",
    "memory_usage_mb": "Peak memory usage"
  }
}
