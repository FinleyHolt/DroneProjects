<?xml version="1.0" ?>
<!--
  Flyby F-11 Forest ISR Training World

  A realistic forest environment for ISR (Intelligence, Surveillance, Reconnaissance)
  drone training featuring:
  - 400m x 400m operational area with geofence
  - Dense tree clusters organized as natural forest groves
  - Forest clearings with hiking trails
  - Ranger station area with parking
  - Campsite with tents and campers
  - Stream/creek bed running through the forest
  - Rock formations as natural landmarks

  ISR DETECTION TARGETS (Critical for training):
  - 10 people: hikers on trails, campers at campsite, ranger at station
  - 5 vehicles: parked cars/trucks at trailhead and ranger station
  - Camping equipment: tents at campsite

  NFZ Regions:
  - Wildlife nesting site (protected area)
  - Private property cabin area

  Designed to train detection and tracking of people/vehicles in forested terrain.

  Model Sources: https://app.gazebosim.org/fuel/models (OpenRobotics)
  Person models: Casual female, Standing person, Walking person
-->
<sdf version="1.9">
  <world name="flyby_forest_isr">

    <!-- Physics optimized for training throughput -->
    <physics name="training_physics" type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
      <ode>
        <solver>
          <type>quick</type>
          <iters>50</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
        </constraints>
      </ode>
    </physics>

    <!-- Plugins -->
    <plugin filename="gz-sim-physics-system" name="gz::sim::systems::Physics"/>
    <plugin filename="gz-sim-sensors-system" name="gz::sim::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
    <plugin filename="gz-sim-scene-broadcaster-system" name="gz::sim::systems::SceneBroadcaster"/>
    <plugin filename="gz-sim-user-commands-system" name="gz::sim::systems::UserCommands"/>
    <plugin filename="gz-sim-imu-system" name="gz::sim::systems::Imu"/>
    <plugin filename="gz-sim-air-pressure-system" name="gz::sim::systems::AirPressure"/>
    <plugin filename="gz-sim-magnetometer-system" name="gz::sim::systems::Magnetometer"/>
    <plugin filename="gz-sim-navsat-system" name="gz::sim::systems::NavSat"/>

    <!-- Atmosphere -->
    <atmosphere type="adiabatic">
      <temperature>293.15</temperature>
      <pressure>101325</pressure>
    </atmosphere>

    <!-- Rural location: Northern California forest area -->
    <spherical_coordinates>
      <surface_model>EARTH_WGS84</surface_model>
      <world_frame_orientation>ENU</world_frame_orientation>
      <latitude_deg>38.9072</latitude_deg>
      <longitude_deg>-121.0583</longitude_deg>
      <elevation>450</elevation>
      <heading_deg>0</heading_deg>
    </spherical_coordinates>

    <!-- ============================================ -->
    <!-- LIGHTING PRESETS FOR DOMAIN RANDOMIZATION   -->
    <!-- Default: Morning sun through canopy         -->
    <!-- ============================================ -->

    <!-- Morning sun (default - active) -->
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 150 0 0 0</pose>
      <diffuse>0.85 0.85 0.75 1</diffuse>
      <specular>0.3 0.3 0.25 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.3 0.2 -0.9</direction>
    </light>

    <!-- Ambient fill for forest shadows -->
    <light type="directional" name="ambient_fill">
      <cast_shadows>false</cast_shadows>
      <pose>0 0 150 0 0 0</pose>
      <diffuse>0.2 0.22 0.18 1</diffuse>
      <specular>0 0 0 1</specular>
      <direction>0 0 -1</direction>
    </light>

    <!-- Scene settings -->
    <scene>
      <ambient>0.35 0.38 0.32 1</ambient>
      <background>0.6 0.7 0.85 1</background>
      <shadows>true</shadows>
      <!-- Morning mist for forest (uncomment to enable) -->
      <!--
      <fog>
        <color>0.85 0.88 0.9 1</color>
        <type>linear</type>
        <density>0.015</density>
        <start>80</start>
        <end>400</end>
      </fog>
      -->
    </scene>

    <!-- ============================================ -->
    <!-- GROUND PLANE - Forest floor texture         -->
    <!-- ============================================ -->
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>500 500</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>80</mu>
                <mu2>40</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>500 500</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.25 0.4 0.2 1</ambient>
            <diffuse>0.3 0.45 0.25 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ============================================ -->
    <!-- STREAM/CREEK BED                            -->
    <!-- Runs diagonally SW to NE through forest     -->
    <!-- ============================================ -->

    <!-- Creek water surface -->
    <model name="creek_water">
      <static>true</static>
      <pose>0 0 -0.15 0 0 0.4</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box><size>300 8 0.3</size></box>
          </geometry>
          <material>
            <ambient>0.15 0.3 0.4 0.85</ambient>
            <diffuse>0.2 0.35 0.45 0.85</diffuse>
            <specular>0.4 0.4 0.4 1</specular>
          </material>
        </visual>
      </link>
    </model>

    <!-- Creek banks (raised edges) -->
    <model name="creek_bank_north">
      <static>true</static>
      <pose>-2.5 5 0.1 0 0 0.4</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box><size>300 3 0.5</size></box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box><size>300 3 0.5</size></box>
          </geometry>
          <material>
            <ambient>0.35 0.3 0.2 1</ambient>
            <diffuse>0.4 0.35 0.25 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="creek_bank_south">
      <static>true</static>
      <pose>2.5 -5 0.1 0 0 0.4</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box><size>300 3 0.5</size></box>
