{
  "phase": "phase-02-uav-ontology",
  "required_outputs": {
    "uav_ontology": {
      "path": "ontology/planning_mode/uav_domain.kif",
      "type": "file",
      "description": "Complete UAV domain ontology in KIF format",
      "verification": "file_exists",
      "min_size_kb": 10
    },
    "test_scenarios": {
      "path": "ontology/planning_mode/test_scenarios/",
      "type": "directory",
      "description": "Test mission scenarios for validation",
      "verification": "directory_exists",
      "required_files": [
        "valid_waypoint_mission.kif",
        "invalid_altitude_mission.kif",
        "battery_constraint_mission.kif"
      ]
    },
    "ontology_documentation": {
      "path": "ontology/planning_mode/UAV_ONTOLOGY.md",
      "type": "file",
      "description": "Documentation of ontology structure and design decisions",
      "verification": "file_exists"
    },
    "consistency_check": {
      "path": "ontology/planning_mode/logs/consistency_check.log",
      "type": "file",
      "description": "SUMO consistency verification output",
      "verification": "file_exists"
    }
  },
  "verification_artifacts": {
    "sumo_test_results": {
      "path": "ontology/planning_mode/logs/test_scenarios_results.log",
      "type": "file",
      "description": "Results from running test scenarios through SUMO"
    },
    "axiom_validation": {
      "path": "ontology/planning_mode/logs/axiom_validation.log",
      "type": "file",
      "description": "Verification that safety axioms detect violations"
    }
  },
  "success_indicators": {
    "ontology_loads": "SUMO loads uav_domain.kif without errors",
    "consistency_passes": "No logical inconsistencies detected",
    "safety_axioms_work": "Invalid missions are correctly rejected",
    "test_scenarios_pass": "All test scenarios produce expected results",
    "documentation_complete": "Key concepts and design rationale documented"
  },
  "metrics": {
    "class_count": "Number of UAV classes defined",
    "axiom_count": "Number of safety/constraint axioms",
    "test_scenario_count": "Number of test missions created"
  }
}
