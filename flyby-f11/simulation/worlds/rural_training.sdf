    <model name="trailhead_sign">
      <static>true</static>
      <pose>-70 -135 0 0 0 0</pose>
      <link name="link">
        <visual name="post">
          <pose>0 0 1 0 0 0</pose>
          <geometry>
            <cylinder><radius>0.08</radius><length>2</length></cylinder>
          </geometry>
